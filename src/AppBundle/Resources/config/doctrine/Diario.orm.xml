<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Diario" table="diario">
    <unique-constraints>
      <unique-constraint name="fecha_UNIQUE" columns="fecha"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fecha" type="date" column="fecha" nullable="false"/>
    <field name="inicial" type="decimal" column="inicial" precision="5" scale="2" nullable="true"/>
    <field name="final" type="decimal" column="final" precision="5" scale="2" nullable="true"/>
    <field name="responsable" type="string" column="responsable" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="colectivoId" type="integer" column="colectivo_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
